<article>

  <h2>Others who added <%= @song.artist %> - <%= @song.title %></h2>
  </br></br>

  </br>
  <table class="table table-striped table-hover ">

	<thead>
		<tr>
			<th>Name</th>
			<th>Times Added</th>
		</tr>
	</thead>
    <% @users.each do |user| %>
      <tr>
        <td class="col-md-1"><%= user.first_name  %> <%= user.last_name %></td>
        <td class="col-md-2"><%= user.songs.count  %></td>
      </tr>
    <% end %>
  </table>

<%= link_to "Dashboard", songs_path %>
</article>